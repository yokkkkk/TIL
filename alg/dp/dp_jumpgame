#include <stdio.h>

int dp[7][7];
int jumpCase[7][7]={{2,5,1,6,1,4,1},
					{6,1,1,2,2,9,3},
					{7,2,3,2,1,3,1},
					{1,1,3,1,7,1,2},
					{4,1,2,3,4,1,3},
					{3,3,1,2,3,4,1},
					{1,5,2,9,4,7,9999}};

int main()
{
	int i=0, j=0;
	int jump = 0;

	dp[0][jumpCase[0][0]] = 1;
	dp[jumpCase[0][0]][0] = 1;

	for(i=0; i<7; i++)
		for(j=0; j<7; j++)
		{
			if(!dp[i][j])
				continue;

			jump = jumpCase[i][j];

			if(j+jump < 7)
				dp[i][j+jump]++;

			if(i+jump < 7)
				dp[i+jump][j]++;
		}

	for(i=0; i<7; i++)
	{
		for(j=0; j<7; j++)
			printf("%d ", dp[i][j]);
		puts("");
	}

	return 0;
}
